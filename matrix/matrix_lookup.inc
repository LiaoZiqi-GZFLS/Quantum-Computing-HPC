inline int lookupMatrix(const std::complex<double> d[2][2], std::uint8_t p)
{
    for (int i = 0; i < 192; ++i)
        if (kAll[i].p == p &&
            kAll[i].d00 == d[0][0] &&
            kAll[i].d01 == d[0][1] &&
            kAll[i].d10 == d[1][0] &&
            kAll[i].d11 == d[1][1])
            return i;
    return -1;   // not found
}
